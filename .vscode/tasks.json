    },
    {
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "Clean",
      "presentation": {
        "panel": "dedicated",
        "reveal": "never"
      },
      "script": "clean",
      "type": "npm"
    },
    {
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "isBackground": true,
      "label": "Webpack Watch",
      "presentation": {
        "group": "Build",
        "panel": "dedicated",
        "reveal": "never"
      },
      "problemMatcher": ["$ts-webpack-watch"],
      "script": "webpack:watch",
      "type": "npm"
    },
    {
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "isBackground": true,
      "presentation": {
        "reveal": "never"
      },
      "problemMatcher": {
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*extension build start*$",
          "endsPattern": "^.*extension (build|rebuild) success.*$"
        },
        "fileLocation": "relative",
        "owner": "typescript",
        "pattern": {
          "code": 5,
          "column": 4,
          "file": 1,
          "line": 3,
          "message": 6,
          "regexp": "^([a-zA-Z]\\:/?([\\w\\-]/?)+\\.\\w+):(\\d+):(\\d+): (ERROR|WARNING)\\: (.*)$"
        }
      },
      "script": "vite-dev",
      "type": "npm"
    },
    {
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "problemMatcher": [],
      "script": "vite-build",
      "type": "npm"
    }
  ],
  "version": "2.0.0"
