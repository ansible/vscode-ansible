      "presentation": {
        "panel": "dedicated",
        "reveal": "never"
      },
      "problemMatcher": ["$tsc-watch"],
      "script": "watch",
      "type": "npm"
    },
    {
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "isBackground": true,
      "presentation": {
        "panel": "dedicated",
        "reveal": "never"
      },
      "problemMatcher": ["$tsc-watch"],
      "script": "watch:server",
