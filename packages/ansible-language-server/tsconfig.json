{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./out/server",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ES2020"
  },
  "exclude": ["node_modules", "out", "vscode-ansible"]
}
