{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2016"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./out/server",
    "sourceMap": true,
    "target": "es6",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "exclude": ["node_modules", "out", "vscode-ansible"],
}
