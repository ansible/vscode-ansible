{
  "activationEvents": [
    "onLanguage:ansible"
  ],
  "all": true,
  "bin": "bin/ansible-language-server",
  "categories": [
    "Programming Languages"
  ],
  "contributors": [
    {
      "email": "tomaciazek@gmail.com",
      "name": "Tomasz Maciążek"
    },
    {
      "name": "RedHat Inc."
    },
    {
      "email": "gnalawad@redhat.com",
      "name": "Ganesh B. Nalawade"
    }
  ],
  "dependencies": {
    "@flatten-js/interval-tree": "^2.0.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "antsibull-docs": "^1.1.4",
    "glob": "^13.0.0",
    "ini": "^6.0.0",
    "lodash": "^4.17.21",
    "uuid": "^13.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-languageserver-types": "^3.17.5",
    "vscode-uri": "^3.1.0",
    "yaml": "^2.8.2"
  },
  "description": "Ansible language server",
  "devDependencies": {
    "@types/ini": "^4.1.1",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.2",
    "@types/vscode": "^1.85.0",
    "fuse.js": "^7.1.0",
    "handlebars": "^4.7.8",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "vscode-jsonrpc": "^8.2.1"
  },
  "displayName": "Ansible",
  "engines": {
    "node": ">=20.0",
    "npm": ">=7.11.2"
  },
  "exports": {
    ".": {
      "default": "./out/server/src/server.js",
      "types": "./out/server/src/server.d.ts"
    },
    "./interfaces/extensionSettings": {
      "default": "./out/server/src/interfaces/extensionSettings.js",
      "types": "./out/server/src/interfaces/extensionSettings.d.ts"
    }
  },
  "files": [
    "icon.png",
    "./out/server/src/**/*"
  ],
  "icon": "icon.png",
  "keywords": [
    "ansible",
    "LSP"
  ],
  "license": "MIT",
  "main": "./out/server/src/server.js",
  "name": "@ansible/ansible-language-server",
  "publisher": "RedHat Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansible/vscode-ansible.git"
  },
  "scripts": {
    "//prepare": "Prepare is needed for installation from source",
    "clean": "rimraf out/server && rimraf lib",
    "compile": "tsc -p .",
    "prepack": "yarn compile",
    "prepare": "yarn run compile",
    "test": "vitest run",
    "test-with-ee": "vitest run -t @ee",
    "test-without-ee": "SKIP_PODMAN=1 SKIP_DOCKER=1 vitest run",
    "watch": "tsc --watch -p ."
  },
  "type": "module",
  "types": "./out/server/src/server.d.ts",
  "version": "1.2.4"
}
