{
  "name": "@ansible/ansible-language-server",
  "publisher": "RedHat Inc.",
  "displayName": "Ansible",
  "description": "Ansible language server",
  "license": "MIT",
  "version": "1.2.3",
  "contributors": [
    {
      "name": "Tomasz Maciążek",
      "email": "tomaciazek@gmail.com"
    },
    {
      "name": "RedHat Inc."
    },
    {
      "name": "Ganesh B. Nalawade",
      "email": "gnalawad@redhat.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansible/vscode-ansible.git"
  },
  "icon": "icon.png",
  "engines": {
    "node": ">=14.0",
    "npm": ">=7.11.2"
  },
  "bin": "bin/ansible-language-server",
  "main": "./out/server/src/server.js",
  "types": "./out/server/src/server.d.ts",
  "files": [
    "icon.png",
    "./out/server/src/**/*"
  ],
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:ansible"
  ],
  "keywords": [
    "ansible",
    "LSP"
  ],
  "dependencies": {
    "@flatten-js/interval-tree": "^1.1.3",
    "antsibull-docs": "^1.0.2",
    "glob": "^10.4.5",
    "ini": "^5.0.0",
    "lodash": "^4.17.21",
    "uuid": "^10.0.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-uri": "^3.0.8",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/glob": "^8.1.0",
    "@types/ini": "^4.1.1",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.8",
    "@types/node": "^20.14.13",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "chai": "^4.5.0",
    "fuse.js": "^7.0.0",
    "handlebars": "^4.7.8",
    "mocha": "^10.7.0",
    "npm-check-updates": "^17.1.3",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "clean": "rimraf out/server && rimraf lib",
    "compile": "tsc -p .",
    "prepack": "yarn compile",
    "//prepare": "Prepare is needed for installation from source",
    "prepare": "yarn run compile",
    "watch": "tsc --watch -p .",
    "test": "sh -c \"nyc mocha ${MOCHA_OPTS:-}\"",
    "test-with-ee": "sh -c \"nyc mocha --grep @ee ${MOCHA_OPTS:-}\"",
    "test-without-ee": "sh -c \"SKIP_PODMAN=1 SKIP_DOCKER=1 nyc mocha --grep @ee --invert ${MOCHA_OPTS:-}\""
  },
  "all": true
}
