{
  "compilerOptions": {
    "composite": true,
    "target": "ES2019",
    "lib": ["ES2019"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "out/client",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src", "test"],
  "exclude": ["node_modules", "test-resources"]
}
